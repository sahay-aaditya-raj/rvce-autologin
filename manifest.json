{
    "manifest_version": 2,
    "name": "AutoLogin CollegeWifi",
    "version": "1.0",
    "description": "Logins automatically to the Internet",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "browser_action": {
      "default_icon": "login.png"
    },
    "icons": {
      "48": "login.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*:1003/*"],
        "js": ["content.js"]
      }
    ]
    
  }
  